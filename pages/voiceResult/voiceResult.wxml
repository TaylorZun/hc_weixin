<!-- pages/voiceResult/voiceResult.wxml -->
<view class="container">
    <view class="confirm">你输入的是不是</view>
    <view class="confirm2">今天9:30测的血糖是5.6</view>
    <view class="num">5.6</view>
    <view class="danwei">mmol/L</view>
    <view class="hr"></view>
    <view class="timedian">
        <view class="text1">时间段</view>
        <view class="text2">早餐后 ></view>
    </view>
    <view class="hr"></view>
    <view class="btn">
        <button type="primary" bindtap="confirm">确认录入</button>
    </view>
    <view class="btn">
        <button type="primary" plain="{{true}}" bindtap="quxiao">重新录入</button>
    </view>
</view>